{
<ENCUESTA_OPEN: "<encuesta>">
|<ENCUESTA_CLOSE: "</encuesta>">
|<PREGUNTA_OPEN: "<pregunta>">
|<PREGUNTA_CLOSE: "</pregunta>">
|<IDPREGUNTA_OPEN: "<idpregunta>">
|<IDPREGUNTA_CLOSE: "</idpregunta>">

|<ETIQUETA_OPEN: "<etiqueta>">
