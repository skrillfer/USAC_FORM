|<PARAMETRO_CLOSE: "</parametro>">


|<TIPO_OPEN: "<tipo>">
|<TIPO_CLOSE: "</tipo>">
|<VALOR: "~" (~["~"])* "~">

}



void Inicio() :
{}
{
    Encuesta() <EOF>
}

void Encuesta() :
{}
{
    <ENCUESTA_OPEN>(Fila())*<ENCUESTA_CLOSE>
}

void Fila():
{}
{
    <PREGUNTA_OPEN> Pregunta() <PREGUNTA_CLOSE> //o agrupacion o preguntas
    {
        /*Se crea la pregunta*/  
        //Pregunta preg 
        Pregunta n_pre = generador.generar_Pregunta();
    }
}

void Pregunta():
{}
{
    (Columna())*
}

void Columna():
{}
{
    Tipo()
}

/*----------------------------___COLUMNAS___----------------------------------*/
void Tipo():
{Token t;}
{
    <TIPO_OPEN> t=<VALOR> <TIPO_CLOSE> {System.out.println(t.image); }
}

void IdPregunta():
{Token t;}
{
    <IDPREGUNTA_OPEN> t=<VALOR> <IDPREGUNTA_CLOSE> {System.out.println(t.image); }
}

void Calculo():
{Token t;}
{
    <CALCULO_OPEN> t=<VALOR> <CALCULO_CLOSE> {System.out.println(t.image); }
}


void Etiqueta():
{Token t;}
{
    <ETIQUETA_OPEN> t=<VALOR> <ETIQUETA_CLOSE> {System.out.println(t.image); }
}

